{
  "ConversionOptions": {
    "EmbedJavascript": false,
    "ProcessAnnotation": true,
    "ProcessOutline": false,
    "Printing": false,
    "BgFormat": "svg",
    "SvgNodeCountLimit": 100,
    "DecomposeLigature": true,
    "Tounicode": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ssZ ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*"
}
